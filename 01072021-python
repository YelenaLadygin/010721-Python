#12
x=10
num10pow = [x ** y for y in range(1, 10)]
print(num10pow)

#11
def Convert(string):
    li = list(string.split(" "))
    return li

mstr = 'i love to eat ice cream in the beach'
print(Convert(mstr.upper()))

example = 'i love to eat ice cream in the beach'
count = {}
for i in example.split():
    count[i] = len(i)
print(count)

