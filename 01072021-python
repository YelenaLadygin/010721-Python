#Targil3
#9
def getinRange (max1,min1):
   for x in range(min1,max1) :
       x = int(input('enter the  number:'))
       if x > min1 and x < max1:
        print(' is valid', x)
        return x
       else  :
         print ('invalid number')

getinRange(133,9)

#10

def minimum3numbers (a,b,c) :
    return min(a,b,c)

x= minimum3numbers(234,76,2)
print(x)

